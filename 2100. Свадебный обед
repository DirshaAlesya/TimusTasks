count = int(input())
my_list = []
for i in range(count):
    answer = str(input())
    my_list.append(answer)
result = 0
for i in my_list:
    if "+one" in i:
        result = result + 2
    else:
        result = result + 1
dollars = 0
if result == 11:
    dollars += 13 * 100 + 100
else:
    dollars += result * 100 + 200
print(dollars)
