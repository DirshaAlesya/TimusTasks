n1 = int(input())
s1 = input()
n2 = int(input())
s2 = input()
n3 = int(input())
s3 = input()
m1 = set()
m2 = set()
m3 = set()
for i in s1.split():
    m1.add(int(i))
for i in s2.split():
    m2.add(int(i))
for i in s3.split():
    m3.add(int(i))
result = []
for i in m1:
    if i in m2 and i in m3:
        result.append(i)
print(len(result))
